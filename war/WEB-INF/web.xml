		<servlet-class>datastoreEntities.SolutionPathServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SolutionPathServlet</servlet-name>
		<url-pattern>/SolutionPathServlet</url-pattern>
	</servlet-mapping>

	<security-constraint>
		<display-name>SecurityConstraint</display-name>
		<web-resource-collection>
			<web-resource-name>WRCollection</web-resource-name>
			<url-pattern>/sign_up</url-pattern>
			<http-method>GET</http-method>
			<http-method>POST</http-method>
		</web-resource-collection>
		<auth-constraint>
			<role-name>*</role-name>
		</auth-constraint>
		<user-data-constraint>
			<transport-guarantee>CONFIDENTIAL</transport-guarantee>
		</user-data-constraint>
	</security-constraint>
	<login-config>
		<auth-method>FORM</auth-method>
		<form-login-config>
			<form-login-page>/sign_up</form-login-page>
			<form-error-page>/sign_up</form-error-page>
		</form-login-config>
	</login-config>
	<security-role>
		<role-name>*</role-name>
	</security-role>

</web-app>