	</servlet-mapping>

	<servlet>
		<servlet-name>sign_up</servlet-name>
		<jsp-file>/WEB-INF/sign_up.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>sign_up</servlet-name>
		<url-pattern>/sign_up</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>contact_us</servlet-name>
		<jsp-file>/WEB-INF/contact_us.jsp</jsp-file>
