	<servlet>
		<servlet-name>SaveNewPlayer</servlet-name>
		<servlet-class>datastoreEntities.SaveNewPlayer</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SaveNewPlayer</servlet-name>
