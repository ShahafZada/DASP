		<url-pattern>/SaveNewMap</url-pattern>
	</servlet-mapping>
	
		<servlet>
		<servlet-name>CreateMapServlet</servlet-name>
		<servlet-class>datastoreEntities.CreateMapServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateMapServlet</servlet-name>
		<url-pattern>/CreateMap</url-pattern>
