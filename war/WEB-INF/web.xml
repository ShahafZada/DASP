	<servlet-mapping>
		<servlet-name>SignInServlet</servlet-name>
		<url-pattern>/sign_in</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>GetDataServlet</servlet-name>
		<servlet-class>datastoreEntities.GetDataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetDataServlet</servlet-name>
		<url-pattern>/GetData</url-pattern>
	</servlet-mapping>
	
	<servlet>
